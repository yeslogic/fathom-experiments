format u24be :=
  let b0 <- byte;
  let b1 <- byte;
  let b2 <- byte;
  #pure
    #Int64
    (#int64-logical-or
       (#int64-logical-or
          (#int64-logical-shl b0 16)
          (#int64-logical-shl b1 16))
       b2);

format u8 := byte;

format u32be :=
  let b0 <- byte;
  let b1 <- byte;
  let b2 <- byte;
  let b3 <- byte;
  #pure
    #Int64
    (#int64-logical-or
       (#int64-logical-or
          (#int64-logical-or
             (#int64-logical-shl b0 24)
             (#int64-logical-shl b1 16))
          (#int64-logical-shl b2 8))
       b3);

type Main {
  counter : #Int64;
  random : #List #Int64;
  timestamp : #Int64;
}

format main :=
  let timestamp <- u32be;
  let random <- #repeat-len 5 u8;
  let counter <- u24be;
  #pure
    Main
    (Main { counter := counter; random := random; timestamp := timestamp });
