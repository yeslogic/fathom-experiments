format u64le :=
  let b0 <- byte;
  let b1 <- byte;
  let b2 <- byte;
  let b3 <- byte;
  let b4 <- byte;
  let b5 <- byte;
  let b6 <- byte;
  let b7 <- byte;
  #pure
    #Int
    (#int64-logical-or
       (#int64-logical-or
          (#int64-logical-or
             (#int64-logical-or
                (#int64-logical-or
                   (#int64-logical-or
                      (#int64-logical-or b0 (#int64-logical-shl b1 8))
                      (#int64-logical-shl b2 16))
                   (#int64-logical-shl b3 24))
                (#int64-logical-shl b4 32))
             (#int64-logical-shl b5 40))
          (#int64-logical-shl b6 48))
       (#int64-logical-shl b7 56));

format u64be :=
  let b0 <- byte;
  let b1 <- byte;
  let b2 <- byte;
  let b3 <- byte;
  let b4 <- byte;
  let b5 <- byte;
  let b6 <- byte;
  let b7 <- byte;
  #pure
    #Int
    (#int64-logical-or
       (#int64-logical-or
          (#int64-logical-or
             (#int64-logical-or
                (#int64-logical-or
                   (#int64-logical-or
                      (#int64-logical-or
                         (#int64-logical-shl b0 56)
                         (#int64-logical-shl b1 48))
                      (#int64-logical-shl b2 40))
                   (#int64-logical-shl b3 32))
                (#int64-logical-shl b4 24))
             (#int64-logical-shl b5 16))
          (#int64-logical-shl b6 8))
       b7);

